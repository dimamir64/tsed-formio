{
  "name": "@tsed/root",
  "version": "1.11.2",
  "description": "",
  "author": "Romain Lenzotti",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/TypedProject/tsed-formio.git"
  },
  "bugs": {
    "url": "https://github.com/TypedProject/tsed-formio/issues"
  },
  "homepage": "https://github.com/TypedProject/tsed-formio",
  "scripts": {
    "postinstall": "lerna run build --stream",
    "configure": "monorepo ci configure",
    "test": "lerna run test --stream",
    "test:e2e": "lerna run test:e2e",
    "lint": "yarn eslint --config .eslintrc --cache --quiet \"**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "build": "monorepo build",
    "publish": "monorepo publish --dry-run",
    "start": "lerna run start --stream --parallel",
    "start:storybook": "lerna run start:storybook --scope=@tsed/storybook --stream",
    "build:storybook": "yarn build:tailwind && lerna run build:storybook --stream --scope @tsed/storybook",
    "build:tailwind": "lerna run build:tailwind --stream --scope @tsed/tailwind",
    "publish:storybook": "yarn build:storybook && monorepo publish ghpages",
    "release": "semantic-release",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "choices.js": "10.1.0",
    "connected-react-router": "6.9.2",
    "formiojs": "4.14.7",
    "lerna": "4.0.0",
    "lodash": "4.17.21",
    "history": "5.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.8",
    "react-router": "5.3.1",
    "react-router-dom": "5.3.1",
    "react-table": "7.7.0",
    "redux-thunk": "2.4.1",
    "tooltip.js": "1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@craco/craco": "6.4.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@tsed/monorepo-utils": "1.17.8",
    "@types/classnames": "2.2.11",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "27.0.2",
    "@types/json2csv": "^5.0.3",
    "@types/lodash": "4.14.168",
    "@types/node": "^16.11.3",
    "@types/prop-types": "15.7.3",
    "@types/react": "^18.0.9",
    "@types/react-custom-scrollbars": "^4.0.10",
    "@types/react-dom": "^18.0.3",
    "@types/react-redux": "^7.1.24",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-table": "^7.7.11",
    "@types/ejs": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cross-env": "7.0.3",
    "is-ci": "^3.0.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "postcss": "7.0.39",
    "postcss-nested": "4.2.3",
    "autoprefixer": "9.8.8",
    "prettier": "2.6.2",
    "microbundle": "0.13.0",
    "react-scripts": "4.0.3",
    "semantic-release": "19.0.2",
    "typescript": "4.6.4"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "packages/*/**/webpack-dev-server",
      "**/babel-loader",
      "**/babel-jest"
    ]
  },
  "monorepo": {
    "productionBranch": "master",
    "developBranch": "master",
    "npmAccess": "public",
    "ghpages": {
      "dir": "./packages/storybook/dist",
      "url": "https://github.com/TypedProject/tsed-formio.git",
      "branch": "gh-pages",
      "cname": "formio.tsed.io"
    }
  }
}
